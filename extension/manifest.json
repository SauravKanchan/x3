{
  "manifest_version": 3,
  "name": "X3",
  "description": "test description",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["https://x.com/*", "https://twitter.com/*"],
      "js": ["./popup.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["dist/*.*"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "declarativeNetRequest",
    "browsingData"
  ]
}
